  {
    "name": "Zeplin open in app",
    "description": "Redirects Zeplin web links to zpl:// app protocol",
    "version": "0.1",
    "manifest_version": 2,
    "background": {
      "scripts": ["background.js"]
    },
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "*://app.zeplin.io/*"
    ]
  }